{
  "name": "solstatus",
  "version": "1.7.1",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "rwsdk dev",
    "build": "rwsdk build",
    "start": "rwsdk start",
    "deploy": "rwsdk deploy",
    "db:create": "wrangler d1 execute solstatus-local --local --command='SELECT 1'",
    "db:create:prod": "wrangler d1 create solstatus-prod",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply DB --local",
    "db:migrate:prod": "wrangler d1 migrations apply DB --env production --remote",
    "db:seed": "tsx test/seed.ts",
    "db:setup": "pnpm run db:create && pnpm run db:generate && pnpm run db:migrate && pnpm run db:seed",
    "db:studio": "drizzle-kit studio",
    "db:studio:prod": "APP_ENV=production drizzle-kit studio",
    "cf-typegen": "wrangler types --env-interface CloudflareEnvGen cloudflare-env-gen.d.ts",
    "format": "biome check . --write"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accessible-icon": "^1.1.7",
